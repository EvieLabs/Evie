{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "../dist",
    "rootDir": ".",
    "baseUrl": ".",
    "jsx": "react",
    "noEmit": true,
    "paths": {
      "#root/*": ["*"],
      "#types/*": ["types/*"],
      "#utils/*": ["utils/*"],
      "#classes/*": ["classes/*"],
      "#constants/*": ["constants/*"],
      "#reacord/*": ["lib/reacord/library/*"]
    }
  },
  "include": ["./**/*.ts", "./**/*.tsx"]
}
