{
  "name": "@evie/park",
  "description": "Dog park full of micro services",
  "version": "0.0.0",
  "main": "dist/index.js",
  "private": true,
  "imports": {
    "#routes/*": "./dist/routes/*.js"
  },
  "_moduleAliases": {
    "environment": "dist/environment/index.js",
    "modules": "dist/modules/index.js"
  },
  "scripts": {
    "dev": "yarn clean && tsc-watch -p src --onSuccess \"node .\"",
    "start": "node dist/index.js",
    "build": "yarn clean && tsc -p src",
    "lint": "yarn eslint --fix --ext ts src",
    "clean": "yarn rimraf dist"
  },
  "author": "Tristan Camejo <contact@tristancamejo.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@mgcrea/fastify-request-logger": "^0.6.1",
    "@sapphire/fetch": "^2.4.0",
    "@sapphire/pieces": "^3.3.4",
    "@sentry/integrations": "^7.2.0",
    "@sentry/node": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "canvas": "^2.9.1",
    "dotenv": "^16.0.1",
    "fastify": "^4.1.0",
    "module-alias": "^2.2.2",
    "node-cache": "^5.1.2",
    "nodejs-assistant": "^0.8.0",
    "openai": "^3.0.0",
    "pino-pretty": "^8.1.0",
    "puppeteer": "^14.4.1",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@types/module-alias": "^2",
    "@types/node": "^17.0.21",
    "rimraf": "^3.0.2"
  }
}
